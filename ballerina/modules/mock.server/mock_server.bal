    # http:DefaultStatusCodeResponse (The request has failed.)
    resource function post users/[UserIdMatchesAuthenticatedUser id]/likes(@http:Payload UsersLikesCreateRequest payload) returns UsersLikesCreateResponse|ErrorProblemDefault {
        return {
            "data": {
                "liked": true
            }
        };
    }

    # Causes the User (in the path) to repost the specified Post.
    #
    # + id - The ID of the authenticated source User that is requesting to repost the Post
    # + return - returns can be any of following types 
    # http:Ok (The request has succeeded)
    # http:DefaultStatusCodeResponse (The request has failed.)
    resource function post users/[UserIdMatchesAuthenticatedUser id]/retweets(@http:Payload UsersRetweetsCreateRequest payload) returns UsersRetweetsCreateResponse|ErrorProblemDefault {
